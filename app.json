{
  "expo": {
    "name": "Disney Clone",
    "slug": "disney-clone-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconn.png",
    "scheme": "disneyapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      "expo-local-authentication",
      [
        "expo-router", 
        {
          "origin": "https://disney-clone.app/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}